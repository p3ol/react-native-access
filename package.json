{
  "name": "@poool/react-native-access",
  "version": "1.1.1",
  "description": "Poool Access, but for React Native",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-access-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "yarn lint && yarn tsc && bob build",
    "release": "commit-and-tag-version && git push --follow-tags",
    "release:dry": "commit-and-tag-version --dry-run"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p3ol/react-native-access.git"
  },
  "author": "Ugo Stephant <ugo@poool.fr> (https://github.com/p3ol)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/p3ol/react-native-access/issues"
  },
  "homepage": "https://github.com/p3ol/react-native-access#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@junipero/core": "3.12.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "20.4.1",
    "@eslint/compat": "1.4.1",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@react-native-community/cli": "20.1.1",
    "@react-native/babel-preset": "0.83.2",
    "@react-native/eslint-config": "0.83.2",
    "@types/jest": "30.0.0",
    "@types/react": "19.2.14",
    "commitlint": "20.4.1",
    "del-cli": "7.0.0",
    "eslint": "9.39.2",
    "eslint-plugin-ft-flow": "3.0.11",
    "eslint-plugin-prettier": "5.5.5",
    "jest": "30.2.0",
    "lefthook": "2.1.1",
    "react": "19.2.4",
    "react-native": "0.83.2",
    "react-native-builder-bob": "0.40.18",
    "turbo": "2.8.9",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@4.12.0",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RNAccessViewSpec",
    "type": "all",
    "jsSrcsDir": "src",
    "outputDir": {
      "android": "android/generated",
      "ios": "ios/generated"
    },
    "includesGeneratedCode": true,
    "android": {
      "javaPackageName": "tech.poool.rnaccess"
    },
    "ios": {
      "componentProvider": {
        "PaywallView": "PaywallView"
      }
    }
  },
  "create-react-native-library": {
    "languages": "kotlin-objc",
    "type": "fabric-view",
    "tools": [
      "eslint",
      "jest",
      "lefthook"
    ],
    "version": "0.57.1"
  }
}
